{
  "name": "storefront-backend",
  "version": "1.0.0",
  "description": "A Storefront Backend",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc --esModuleInterop src/server.ts --outDir ./dist",
    "watch": "npm run build -- --watch",
    "lint": "eslint src",
    "test": "set NODE_ENV=test && db-migrate -e test up && npx tsc && jasmine && db-migrate -e test reset",
    "test:watch": "npm run test -- --watch",
    "jasmine": "jasmine"
  },
  "author": "Mohamed Mahmoud AbdElbadie Hasan",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "supertest": "^6.2.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.1",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.3",
    "jest": "^26.4.2",
    "supertest": "^6.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
